{% from "favorite/favorite.njk" import favorite %}
{% from "accordion/accordion.njk" import accordion %}
{% from "auto-bet-btn/auto-bet-btn.njk" import autoBetBtn %}
{% from "set-bet-price/set-bet-price.njk" import setBetPrice %}
{% from "make-bet-btn/make-bet-btn.njk" import makeBetBtn %}
{% from "document-item/document-item.njk" import documentItem %}

<div class="lot-detail">
    <div class="lot-detail__inner">
        <div class="lot-detail-product">
            <div class="lot-detail-product__slider">
                <div class="lot-detail-slider">
                    <div class="lot-detail-slider__main">
                        <div class="swiper js-lot-detail-slider__swiper">
                            <div class="swiper-wrapper">
                                {% for item in range(0, 10) %}
                                    <div class="swiper-slide">
                                        <div class="lot-detail-slider__pic">
                                            <a class="js-lot-detail-slider__fancybox-el" data-fancybox="gallery" data-src="images/lot-detail/pic.jpg">
                                                <picture>
                                                    <img class="lazyload" data-src="images/lot-detail/pic.jpg" alt="pic">
                                                </picture>
                                            </a>
                                        </div>
                                    </div>
                                {% endfor %}
                            </div>
                            <div class="swiper-pagination"></div>
                        </div>
                        
                        {% if not detailWon and not detailNotWon %}
                            <div class="lot-detail-slider__timer">
                                {% include "lot-timer/lot-timer.njk" %}
                            </div>
                        {% endif %}

                        {% if detailWon or detailNotWon %}
                            <div class="lot-detail-slider__label">
                                {% if detailNotWon %}
                                    {% include "label-not-won/label-not-won.njk" %}
                                {% else %}
                                    {% include "label-won/label-won.njk" %}
                                {% endif %}
                            </div>
                            <div class="lot-detail-slider__big-label">
                                {% if detailNotWon %}
                                    {% include "big-label-not-won/big-label-not-won.njk" %}
                                {% else %}
                                    {% include "big-label-won/big-label-won.njk" %}
                                {% endif %}
                            </div>
                        {% endif %}

                        <div class="lot-detail-slider__slide-count {% if detailWon or detailNotWon %}lot-detail-slider__slide-count_bottom-bigger{% endif %}">
                            <span class="js-lot-detail-slider__slide-dynamic-count">1</span>/<span>10</span>
                        </div>
                    </div>
                    <div class="lot-detail-slider__thumbs">
                        {% for item in range(0, 4) %}
                            <div 
                                class="
                                    lot-detail-slider__thumb
                                    {% if loop.last === false %}
                                        js-lot-detail-slider__thumb_change-slide
                                    {% else %}
                                        js-lot-detail-slider__thumb_open-fancybox
                                    {% endif %}
                                    {% if loop.index === 1 %}active{% endif %}
                                "
                            >
                                <picture>
                                    <img class="lazyload" data-src="images/lot-detail/thumb.jpg" alt="slider thumb">
                                </picture>

                                {% if loop.last === true %}
                                    <div class="lot-detail-slider__thumb-total-count">
                                        <span>+6</span>
                                    </div>
                                {% endif %}
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
            <div class="lot-detail-product__desc">
                <div class="lot-detail-product__name">
                    <span>BMW 7 серии 730Ld xDrive 3.0d AT</span>
                    <div class="lot-detail-product__favorite">
                        {{ favorite() }}
                    </div>
                </div>
                <div class="lot-detail-product__lot-info">
                    <div class="lot-detail-product__lot-id">
                        <span class="lot-detail-product__lot-id-icon">
                            <i class="icon-bids"></i>
                        </span>
                        <span>№: 107589</span>
                    </div>
                    <div class="lot-detail-product__lot-info-vert-line"></div>
                    <div class="lot-detail-product__checked">Осмотрено продавцом</div>
                </div>
                <div class="lot-detail-product__properties">
                    <div class="lot-detail-product__property">
                        <div class="lot-detail-product__property-key">Заканчивается:</div>
                        <div class="lot-detail-product__property-value">13.12.2022, 12:20 МСК</div>
                    </div>
                    <div class="lot-detail-product__property">
                        <div class="lot-detail-product__property-key">Начало торгов:</div>
                        <div class="lot-detail-product__property-value">7.12.2022, 12:20 МСК</div>
                    </div>
                    <div class="lot-detail-product__property">
                        <div class="lot-detail-product__property-key">Тип аукциона:</div>
                        <div class="lot-detail-product__property-value">
                            <span>Открытые торги</span>
